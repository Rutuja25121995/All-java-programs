package com.tech.String;

public class Palindrome {

	public static void main(String[] args) {
		String str="nitin";
	String rev="";
		// String str1=str;
		 for(int i=str.length()-1;i>=0;i--) {
			 
			 rev=rev+str.charAt(i);
			 
			 
		 }

		 if(str.equals(rev)) {
			 System.out.println("palindrome");
		 }else {
			 System.out.println("not a palindrome");
		 }
	}

}
