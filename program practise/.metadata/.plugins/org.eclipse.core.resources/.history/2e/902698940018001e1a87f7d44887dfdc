package com.tech.String;

public class Palindrome {

	public static void main(String[] args) {
		String str="nitin1";
		// int length=str.length();
		 String str1=str;
		 for(int i=str.length()-1;i>=0;i--) {
			 System.out.println(str.charAt(i));
		 }

		 if(str1==str) {
			 System.out.println("palindrome");
		 }
	}

}
